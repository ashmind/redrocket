{
    "insertHintOnTab": true,
    "spaceUnits": 2,
    "path": {
        "**.js": {
            "spaceUnits": 4,
            "brackets-git.gitPath": "git",
            "fonts.fontSize": "20px",
            "showLineNumbers": false,
            "hdy.brackets-shell.dark": false,
            "hdy.brackets-shell.trackProject": true,
            "hdy.brackets-shell.shell": "cmd.exe"
        }
    },
    "language": {
        "javascript": {
            "linting.prefer": [
                "JSHint"
            ],
            "linting.usePreferredOnly": true
        }
    }
}